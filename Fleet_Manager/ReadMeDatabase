//Tietokanta

CREATE TABLE [dbo].[AutoTable] (
    [Id]              INT          IDENTITY (1, 1) NOT NULL,
    [merkki]          VARCHAR (50) NULL,
    [malli]           VARCHAR (50) NULL,
    [rekisterinumero] NCHAR (10)   NULL,
    [vuosimalli]      INT          NULL,
    [katsastuspvm]    DATETIME     NULL,
    [koko]            FLOAT (53)   NULL,
    [teho]            FLOAT (53)   NULL,
    PRIMARY KEY CLUSTERED ([Id] ASC)
);



